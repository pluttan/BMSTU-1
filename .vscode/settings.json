{
    // "cSpell.diagnosticLevel": "Error",
    //! workbench settings
    "explorer.sortOrder": "type",
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "terminal.integrated.smoothScrolling": true,
    "terminal.integrated.defaultProfile.osx": "zsh",
    "terminal.integrated.detectLocale": "off",
    "terminal.integrated.localEchoLatencyThreshold": 0,
    "terminal.integrated.shellIntegration.enabled": false,
    "terminal.integrated.cwd": "/",
    "liveshare.allowInvites": "all",
    "liveServer.settings.donotVerifyTags": true,
    "liveshare.allowGuestDebugControl": true,
    "liveshare.increasedGuestLimit": false,
    "liveshare.anonymousGuestApproval": "accept",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "workbench.preferredLightColorTheme": "Default Dark+",
    "workbench.startupEditor": "newUntitledFile",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.colorTheme": "Sublime Material Theme - Dark",
    "workbench.preferredDarkColorTheme": "Monokai Pro (Filter Machine)",
    "workbench.editor.decorations.colors": false,
    "workbench.editor.untitled.hint": "hidden",
    "workbench.fontAliasing": "auto",
    "workbench.editorAssociations": {
        "*.py": "default",
        "*.pdf": "latex-workshop-pdf-hook",
        "*.pptx": "default",
        "*.ipynb": "jupyter-notebook",
        "*.mp3": "default",
        "*.mp4": "analyticsignal.preview-mp4",
        "*.nib": "default",
        "*.ttf": "default"
    },
    "color-highlight.matchWords": true,
    "color-highlight.rgbWithNoFunctionLanguages": [
        "*"
    ],
    "color-highlight.languages": [
        "*"
    ],
    "color-highlight.markerType": "dot-before",
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.indentation": true,
    "editor.fontFamily": "Fira Code, Menlo, Monaco, 'Courier New', monospace",
    "editor.renderControlCharacters": true,
    "editor.mouseWheelZoom": true,
    "editor.formatOnSave": true,
    "editor.cursorSmoothCaretAnimation": true,
    "editor.detectIndentation": false,
    "editor.linkedEditing": true,
    "editor.accessibilitySupport": "off",
    "editor.tabCompletion": "on",
    "editor.guides.bracketPairs": "active",
    "editor.unicodeHighlight.invisibleCharacters": false,
    "editor.guides.bracketPairsHorizontal": "active",
    "editor.guides.highlightActiveIndentation": true,
    "editor.unicodeHighlight.nonBasicASCII": false,
    "editor.fontSize": 24,
    "editor.lineHeight": 0,
    "editor.stickyScroll.enabled": true,
    "editor.smoothScrolling": true,
    "editor.guides.highlightActiveBracketPair": true,
    "editor.fontLigatures": true,
    "editor.largeFileOptimizations": false,
    "editor.cursorBlinking": "expand",
    "editor.acceptSuggestionOnEnter": "off",
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "editor.rulers": [
        79,
        120
    ],
    "editor.language.brackets": [
        [
            "[",
            "]"
        ],
        [
            "{",
            "}"
        ]
    ],
    "editor.language.colorizedBracketPairs": [
        [
            "[",
            "]"
        ],
        [
            "(",
            ")"
        ],
        [
            "{",
            "}"
        ]
    ],
    "editor.suggestSelection": "first",
    "files.defaultLanguage": "py, html, tex, cpp, ino, css, js",
    "files.autoSave": "onFocusChange",
    "files.associations": {
        "*.tex": "tex",
        "*.pyde": "pyprocessing",
        "*.sql": "mysql",
        "*.ipynb": "jupyter",
        "*.pro": "profile",
        "*.md": "markdown",
        "*.pdf": "pdffile"
    },
    //! code runner
    "code-runner.defaultLanguage": "tex",
    "code-runner.saveAllFilesBeforeRun": true,
    "code-runner.runInTerminal": false,
    "code-runner.executorMap": {
        "tex": "~/cerTex '$dir$fileName'"
    },
    "code-runner.executorMapByFileExtension": {
        ".tex": "cerTex '$dir$fileName'",
    },
    //! commentAnchors settings
    "commentAnchors.tags.anchors": {
        "ANCHOR": {
            "iconColor": "default",
            "highlightColor": "#A8C023",
            "scope": "file"
        },
        "TODO": {
            "iconColor": "blue",
            "highlightColor": "#3ea8ff",
            "scope": "workspace"
        },
        "FIXME": {
            "iconColor": "red",
            "highlightColor": "#F44336",
            "scope": "workspace"
        },
        "STUB": {
            "iconColor": "purple",
            "highlightColor": "#BA68C8",
            "scope": "file"
        },
        "NOTE": {
            "iconColor": "orange",
            "highlightColor": "#FFB300",
            "scope": "file"
        },
        "REVIEW": {
            "iconColor": "green",
            "highlightColor": "#64DD17",
            "scope": "workspace"
        },
        "SECTION": {
            "iconColor": "blurple",
            "highlightColor": "#896afc",
            "scope": "workspace",
            "behavior": "region"
        },
        "SWOR": {
            "iconColor": "#FFDD17",
            "highlightColor": "#FFDD17",
            "scope": "file"
        },
        "SUBSECTION": {
            "iconColor": "#64DD17",
            "highlightColor": "#64DD17",
            "scope": "workspace",
            "behavior": "region"
        },
        "LINK": {
            "iconColor": "#2ecc71",
            "highlightColor": "#2ecc71",
            "scope": "workspace",
            "behavior": "link"
        }
    },
    //! cSpell settings
    "cSpell.language": "ru-RU,ru",
    "cSpell.dictionaryDefinitions": [],
    "cSpell.languageSettings": [],
    "cSpell.suggestionNumChanges": 5,
    "cSpell.enableFiletypes": [
        "tex"
    ],
    "cSpell.enabledLanguageIds": [
        "tex"
    ],
    "cSpell.userWords": [
        "pluttan"
    ],
    "cSpell.dictionaries": [
        "tex"
    ],
    "cSpell.noSuggestDictionaries": [],
    "cSpell.maxNumberOfProblems": 1000,
    "cSpell.words": [
        "alpha",
        "BMSTU",
        "center",
        "circ",
        "clearpage",
        "degree",
        "Docs",
        "dotitle",
